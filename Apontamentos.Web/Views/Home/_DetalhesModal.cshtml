@model List<Apontamentos.Web.Models.DetalheApontamentoViewModel>
@{
    Layout = null;
}
@if (Model.Count > 0)
{
    <table id="DetalhesApontamentosGrid" class="table" cellspacing="0">
        <thead>
            <tr>
                <th>Código</th>
                <th>Tarefa</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.externalId</td>
                    <td>@item.titlte</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <div class="row">
        <h7 id="lblTitulo">NENHUMA TAREFA ENCONTRADA.</h7>
    </div>
}